@charset "EUC-KR";

#logo
{
	cursor: pointer;
}
#first
{
	position:absolute;
	top:10px;
	right:50px;
}
#first button
{
	font-weight:bold;
	font-size:20px;
	color:#1E3269;
}
.backimg
{	
	margin-top:50;
	width:100%;
	height:700px;
	background-image: url(../images/background3.jpg);
	background-size: cover;
	background-repeat: no-repeat;
}
.content
{
	position:absolute;
	top:50%;
	left:10%;
	transform:traslate(-50%, -50%);
	color:white;
	z-index:2;
	text-align:center;
}
#maincontent
{
	font-weight:bold;
	font-size:60px;
}
span
{
	font-weight:bold;
	font-size:30px;
}
.contents
{

	width :100%;
	height : 10px;
	display: flex;
	justify-content: center;
	align-items: center;
	z-index:1;
}
ul
{
	list-style: none;
}
li
{
	display: inline-block;
	text-align: center;
	padding: 250px 250px 0 200px;
}
a
{	
	font-weight:bold;
	font-size:20px;
	color:#1E3269;
}