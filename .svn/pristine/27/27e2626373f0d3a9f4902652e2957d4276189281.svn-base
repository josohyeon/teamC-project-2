@charset "EUC-KR";

body
{
	background-color: #343A40;
}
#first
{
	position:absolute;
	top:10px;
	right:50px;
}
#first button
{
	font-weight:bold;
	font-size:20px;
	color:white;
}
.contents
{
	width :100%;
	height : 10px;
	display: flex;
	justify-content: center;
	align-items: center;
	z-index:1;
}
ul
{
	list-style: none;
}
li
{
	display: inline-block;
	text-align: center;
	padding: 60px 200px 0 200px;
}
a
{	
	font-weight:bold;
	font-size:20px;
	color:white;	
}
a:hover
{
	color:#5ABEFF
}
.info
{
	position:absolute;
	top:5%;
	width:100%;
	height:950px;
	background-color: white;
	padding-top:50px;
}
table
{
	width:900px;
	height:850px;
	margin:auto;
}
th
{
	text-align:left;
	width:250px;
}
td
{
	text-align:center;
}