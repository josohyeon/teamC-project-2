@charset "EUC-KR";

body
{
	background-color: #FFBE0A;
}
#first
{
	position:absolute;
	top:10px;
	right:50px;
}
#first button
{
	font-weight:bold;
	font-size:20px;
	color:black;
}
.contents
{
	width :100%;
	height : 10px;
	display: flex;
	justify-content: center;
	align-items: center;
	z-index:1;
}
ul
{
	list-style: none;
}
li
{
	display: inline-block;
	text-align: center;
	padding: 60px 200px 0 200px;
}
a
{	
	font-weight:bold;
	font-size:20px;
	color:black;	
}
a:hover
{
	color:white;
}
.info
{
	position:absolute;
	top:5%;
	width:100%;
	height:950px;
	background-color: white;
	padding-top:50px;
	overflow:auto;
}
th, td
{
	text-align:center;
}
.trainerv
{
	display:none;
}