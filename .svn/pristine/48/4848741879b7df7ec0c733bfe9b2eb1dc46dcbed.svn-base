@charset "EUC-KR";
@import url(https://cdn.jsdelivr.net/gh/moonspam/NanumSquare@1.0/nanumsquare.css);
body
{
	background-color: #343A40;
	font-family: 'NanumSquare', sans-serif;
}
#first
{
	position:absolute;
	top:10px;
	right:50px;
}
#first button
{
	font-weight:bold;
	font-size:20px;
	color:white;
}
.contents
{
	width :100%;
	height : 10px;
	display: flex;
	justify-content: center;
	align-items: center;
	z-index:1;
}
ul
{
	list-style: none;
}
li
{
	display: inline-block;
	text-align: center;
	padding: 60px 200px 0 200px;
}
a
{	
	font-weight:bold;
	font-size:20px;
	color:white;	
}
a:hover
{
	color:#5ABEFF
}
.info
{
	position:absolute;
	top:5%;
	width:100%;
	height:950px;
	background-color: white;
	padding-top:100px;
}
td
{
	text-align:center;
}
th, td
{
	font-size:17px;
}