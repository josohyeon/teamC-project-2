@charset "EUC-KR";

body
{
	background-color: #343A40;
	-ms-overflow-style: none;
}

/*::-webkit-scrollbar { display: none; } 웹페이지 스크롤바 안보이게함.*/

#first
{
	position:absolute;
	top:10px;
	right:50px;
}
#first button
{
	font-weight:bold;
	font-size:20px;
	color:white;
}
.content
{

	width :100%;
	height : 10px;
	display: flex;
	justify-content: center;
	align-items: center;
	z-index:1;
}
ul
{
	list-style: none;
}
li
{
	display: inline-block;
	text-align: center;
	padding: 60px 250px 0 200px;
}
span
{
	color:white;
}
a
{	
	font-weight:bold;
	font-size:20px;
	color:white;
}
a:hover
{
	color:#5ABEFF
}
.list
{
	position:absolute;
	top:5%;
	width:100%;
	height:950px;
	background-color: white;
	padding-top:100px;
	overflow:auto;
}
.listcont
{
	position:absolute;
	left:15%;
	width:30%;
}
#divDetail_1
{
	display:none;
	width:500px;
	-ms-overflow-style: none;
}
.exlist
{
	display:none;
}
#leftimg
{
	position:fixed;
	float:left;
	left:2%;
	width:250px;
	height:600px;
	background-image: url(../images/nike4.jpg);
	background-repeat: no-repeat;
}
#angle_up
{
	position:absolute;
	bottom:6%;
	right:5%;
}