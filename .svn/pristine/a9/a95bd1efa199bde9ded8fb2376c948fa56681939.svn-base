@charset "EUC-KR";
@import url(https://cdn.jsdelivr.net/gh/moonspam/NanumSquare@1.0/nanumsquare.css);
body
{
	font-family: 'NanumSquare', sans-serif;
}
#divResult
{
	display:none; 
	left:38%; 
	top:40%; 
	width:500px; 
	height:200px; 
	position:absolute; 
	border: 5px solid gold; 
	border-radius:10px; 
	background-color: white;
}
